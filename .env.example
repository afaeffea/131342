PORT=3000

# Любая длинная строка, в проде обязательно поменяйте
SESSION_SECRET=super-secret-session-key-change-me-in-production

# PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=lawyer_chatbot
DB_USER=postgres
DB_PASSWORD=your_postgres_password

# URL вебхука n8n, который будет обрабатывать сообщения чата
# Пример: https://n8n.your-domain.com/webhook/lawyer-chat
N8N_WEBHOOK_URL=https://example.com/your-n8n-webhook

# Environment (development, production)
NODE_ENV=development

